<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kinmanlui.controller.TestNameController">
   <children>
      <Label contentDisplay="TOP" prefHeight="17.0" prefWidth="340.0" text="Please enter the test name for this class:" />
      <TextField fx:id="textField" onKeyPressed="#onKeyPressed" />
      <HBox nodeOrientation="RIGHT_TO_LEFT" spacing="5.0">
         <children>
            <Button mnemonicParsing="false" text="Submit " fx:id="submitButton" onMousePressed="#submitOnMousePressed"/>
            <Button mnemonicParsing="false" text="Cancel" fx:id="cancelButton" onMousePressed="#cancelOnMousePressed"/>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
